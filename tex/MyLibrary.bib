@article{livingstonFurtherDevelopingFrith2021,
  title = {Further Developing the {{{\textsc{Frith}}}}{\textsc{\textendash}}{{{\textsc{Happ\'e}}}} Animations: {{A}} Quicker, More Objective, and Web-based Test of Theory of Mind for Autistic and Neurotypical Adults},
  shorttitle = {Further Developing The},
