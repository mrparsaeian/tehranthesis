  file = {/home/master/pj/Zotero/storage/GSPP2KTD/Teoh et al. - 2020 - Attentional priorities drive effects of time press.pdf;/home/master/pj/Zotero/storage/NAKRMGQH/Teoh et al_2020_Attentional priorities drive effects of time pressure on altruistic choice.pdf}
