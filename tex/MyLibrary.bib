@article{mahmoodiEqualityBiasImpairs2015,
  title = {Equality Bias Impairs Collective Decision-Making across Cultures},
  author = {Mahmoodi, Ali and Bang, Dan and Olsen, Karsten and Zhao, Yuanyuan Aimee and Shi, Zhenhao and Broberg, Kristina and Safavi, Shervin and Han, Shihui and Nili Ahmadabadi, Majid and Frith, Chris D. and Roepstorff, Andreas and Rees, Geraint and Bahrami, Bahador},
  year = {2015},
