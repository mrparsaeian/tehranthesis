}

@article{suzukiOnlineExperimentalFramework2018,
  title = {An {{Online Experimental Framework}} for {{Cooperative Relationships With}} a {{Real-Time Decision-Making}} and {{Rewarding Environment}}},
  author = {Suzuki, Reiji and Ito, Momoka and Kodera, Shunnya and Nishimoto, Keita and Arita, Takaya},
