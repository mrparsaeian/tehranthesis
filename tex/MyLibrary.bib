@article{barthPrivacyParadoxInvestigating2017,
  title = {The {{Privacy Paradox}} \textendash{} {{Investigating Discrepancies}} between {{Expressed Privacy Concerns}} and {{Actual Online Behavior}} \textendash{} {{A Systematic Literature Review}}},
  author = {Barth, Susanne and De Jong, Menno},
  year = {2017},
