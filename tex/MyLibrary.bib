@article{shiCouplingGroupSelection2022,
  title = {Coupling Group Selection and Network Reciprocity in Social Dilemmas through Multilayer Networks},
  author = {Shi, Zhenyu and Wei, Wei and Perc, Matja{\v z} and Li, Baifeng and Zheng, Zhiming},
