  keywords = {\#nosource,attention,Attention,brain cortex,Cerebral Cortex,cognition,Cognition,drawing,human,human experiment,Humans,Memory; Short-Term,neurobiology,physiology,psychological theory,Psychological Theory,Saccades,saccadic eye movement,sensation,Sensation,short term memory,simulation,theoretical study,working memory,writing},
