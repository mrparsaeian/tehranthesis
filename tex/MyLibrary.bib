@article{yunChronologicalReviewEmpirical2019,
  title = {A Chronological Review of Empirical Research on Personal Information Privacy Concerns: {{An}} Analysis of Contexts and Research Constructs},
  shorttitle = {A Chronological Review of Empirical Research on Personal Information Privacy Concerns},
  author = {Yun, Haejung and Lee, Gwanhoo and Kim, Dan J},
  year = {2019},
  month = jun,
  journal = {Information \& Management},
  volume = {56},
